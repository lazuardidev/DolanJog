{
    "version": 2,
    "builds": [
        {
            "src":"api_rec/app.py",
            "use":"@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"api_rec/app.py"
        }
    ],
    "functions": {
        "api/*.py": {
          "runtime": "vercel-python@0.5.3"
        }
      }
}